<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daisy.bangsen.dao.LeadgerDao">

    <select id="selectByParam" parameterType="HashMap" resultType="com.daisy.bangsen.entity.financial.Ledger">
        SELECT *from t_ledger
        where 1=1
        <if test="name != null and '' != name">
            and name like "%"#{name}"%"
        </if>
    </select>

</mapper>
